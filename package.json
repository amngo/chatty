{
  "name": "server",
  "version": "1.0.0",
  "description": "Back end for handling chat messages.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "yarn --cwd ./client start",
